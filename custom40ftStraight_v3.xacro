            <inertia ixx="0.2" iyy="0.2" izz="0.2"/>
        </inertial>
    </link>
    <joint name="leftFrameJoint" type="fixed">
        <parent link="base_link"/>
        <child link="leftFrame"/>
        <origin xyz="0 -0.63 0.2" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <!-- Right Guide Frame-->
